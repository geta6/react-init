{
  "extends": "airbnb",
  "env": {
    "mocha": true
  },
  "plugins": [
    "import"
  ],
  "settings": {
    "import/parser": "babel-eslint"
  },
  "rules": {
    "object-curly-spacing": [2, "never"],
    "jsx-quotes": [2, "prefer-single"],
    "react/jsx-quotes": 0
  },
  "globals": {
    "__DEV__": true
  }
}
